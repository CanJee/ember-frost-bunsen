<div class='{{labelPlacement}}-label'>
    <label for={{_inputId}} class="alias">{{renderLabel}}</label>
    {{#if required}}
        <div class='required'>Required</div>
    {{/if}}
</div>
<div class='{{labelPlacement}}-input'>
  {{frost-select
    data=data
    disabled=disabled
    onChange=(action "onChange")
    selectedValue=useKey
  }}
</div>
