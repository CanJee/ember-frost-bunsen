{{#if renderLabel}}
  {{#frost-bunsen-section
    className="model-container"
    collapsible=collapsible
    instructions=instructions
    required=isRequired
    title=renderLabel
  }}
    {{frost-bunsen-container
      bunsenId=bunsenId
      bunsenModel=bunsenModel
      bunsenStore=bunsenStore
      cellConfig=cellConfig
      errors=errors
      onChange=onChange
      readOnly=readOnly
      value=value
    }}
  {{/frost-bunsen-section}}
{{else}}
  {{frost-bunsen-container
    bunsenId=bunsenId
    bunsenModel=bunsenModel
    bunsenStore=bunsenStore
    cellConfig=cellConfig
    errors=errors
    onChange=onChange
    readOnly=readOnly
    value=value
  }}
{{/if}}
