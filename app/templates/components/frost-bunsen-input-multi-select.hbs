<label class={{labelWrapperClassName}}>
  {{renderLabel}}
  {{#if required}}
    <div class='required'>Required</div>
  {{/if}}
</label>
<div class={{inputWrapperClassName}}>
  {{frost-multi-select
    data=options
    disabled=disabled
    onInput=onInput
    onChange=(action "onChange")
    placeholder=cellConfig.placeholder
    selectedValue=value
  }}
  {{#if renderErrorMessage}}
    <div class="error">
      {{renderErrorMessage}}
    </div>
  {{/if}}
</div>
