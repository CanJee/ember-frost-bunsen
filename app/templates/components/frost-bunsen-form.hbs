{{#if isInvalid}}
  {{frost-bunsen-validation-result
    model=propValidationResult
  }}
{{else}}
  <form class="form">
    {{#if cellTabs.length}}
      {{#frost-tabs
        onChange=(action "onTabChange")
        selection=(or selectedTabIndex 0)
        tabs=cellTabs
      }}
        {{#each cellTabs as |tab|}}
          {{frost-tab
            alias=tab.alias
            id=tab.id
          }}
        {{/each}}
      {{/frost-tabs}}
    {{/if}}
    {{#each renderView.cells as |cell|}}
      {{frost-bunsen-cell
        bunsenModel=renderModel
        bunsenStore=bunsenStore
        cellConfig=cellConfig
        errors=renderErrors
        onChange=(action "onChange")
        value=renderValue
      }}
    {{/each}}
  </form>
{{/if}}
