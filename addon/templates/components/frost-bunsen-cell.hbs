{{#if showSection}}
  {{#frost-bunsen-section
    collapsible=cellConfig.collapsible
    description=cellConfig.description
    required=isRequired
    title=cellConfig.label
  }}
    {{#if cellConfig.model}}
      {{#if isSubModelObject}}
        {{frost-bunsen-cell
          changeSet=propagatedChangeSet
          bunsenId=cellConfig.bunsenId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=cellConfig
          errors=diffError
          formDisabled=formDisabled
          formHook=formHook
          onChange=onChange
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          showAllErrors=showAllErrors
          value=diffValue
        }}
      {{else if isSubModelArray}}
        {{#if isArrayItem}}
          {{frost-bunsen-array-inline-item
            changeSet=propagatedChangeSet
            bunsenId=nonIndexId
            bunsenModel=subModel
            bunsenView=bunsenView
            cellConfig=cellConfig
            errors=diffError
            formDisabled=formDisabled
            formHook=formHook
            index=index
            onChange=onChange
            readOnly=readOnly
            registerForFormValueChanges=registerForFormValueChanges
            renderers=renderers
            showAllErrors=showAllErrors
            showRemoveButton=false
            sortable=false
            value=diffValue
          }}
        {{else}}
          {{frost-bunsen-array-container
            changeSet=propagatedChangeSet
            bunsenId=cellConfig.bunsenId
            bunsenModel=subModel
            bunsenView=bunsenView
            cellConfig=cellConfig
            errors=diffError
            formDisabled=formDisabled
            formHook=formHook
            onChange=onChange
            readOnly=readOnly
            registerForFormValueChanges=registerForFormValueChanges
            renderers=renderers
            required=required
            showAllErrors=showAllErrors
            value=diffValue
          }}
        {{/if}}
      {{else}}
        {{frost-bunsen-input-wrapper
          changeSet=propagatedChangeSet
          bunsenId=cellConfig.bunsenId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=cellConfig
          errorMessage=errorMessage
          formDisabled=formDisabled
          formHook=formHook
          onChange=onChange
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          required=required
          showAllErrors=showAllErrors
          isDependencyMet=isDependencyMet
          value=renderValue
        }}
      {{/if}}
    {{/if}}
    {{#each cellConfig.children as |child index|}}
      {{frost-bunsen-cell
        changeSet=propagatedChangeSet
        bunsenId=child.bunsenId
        bunsenModel=bunsenModel
        bunsenView=bunsenView
        cellConfig=child
        errors=diffError
        formDisabled=formDisabled
        formHook=formHook
        onChange=onChange
        readOnly=readOnly
        registerForFormValueChanges=registerForFormValueChanges
        renderers=renderers
        showAllErrors=showAllErrors
        value=diffValue
      }}
    {{/each}}
  {{/frost-bunsen-section}}
{{else}}
  {{#if cellConfig.model}}
    {{#if isSubModelObject}}
      {{frost-bunsen-cell
        changeSet=propagatedChangeSet
        bunsenId=cellConfig.bunsenId
        bunsenModel=subModel
        bunsenView=bunsenView
        cellConfig=cellConfig
        errors=diffError
        formDisabled=formDisabled
        formHook=formHook
        onChange=onChange
        readOnly=readOnly
        registerForFormValueChanges=registerForFormValueChanges
        renderers=renderers
        showAllErrors=showAllErrors
        value=diffValue
      }}
    {{else if isSubModelArray}}
      {{#if isArrayItem}}
        {{frost-bunsen-array-inline-item
          changeSet=propagatedChangeSet
          bunsenId=nonIndexId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=cellConfig
          errors=diffError
          formDisabled=formDisabled
          formHook=formHook
          index=index
          onChange=onChange
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          showAllErrors=showAllErrors
          showRemoveButton=false
          sortable=false
          value=diffValue
        }}
      {{else}}
        {{frost-bunsen-array-container
          changeSet=propagatedChangeSet
          bunsenId=cellConfig.bunsenId
          bunsenModel=subModel
          bunsenView=bunsenView
          cellConfig=cellConfig
          errors=diffError
          formDisabled=formDisabled
          formHook=formHook
          onChange=onChange
          readOnly=readOnly
          registerForFormValueChanges=registerForFormValueChanges
          renderers=renderers
          required=required
          showAllErrors=showAllErrors
          value=diffValue
        }}
      {{/if}}
    {{else}}
      {{frost-bunsen-input-wrapper
        changeSet=propagatedChangeSet
        bunsenId=cellConfig.bunsenId
        bunsenModel=subModel
        bunsenView=bunsenView
        cellConfig=cellConfig
        errorMessage=errorMessage
        formDisabled=formDisabled
        formHook=formHook
        onChange=onChange
        readOnly=readOnly
        registerForFormValueChanges=registerForFormValueChanges
        renderers=renderers
        required=required
        showAllErrors=showAllErrors
        isDependencyMet=isDependencyMet
        value=renderValue
      }}
    {{/if}}
  {{/if}}
  {{#each cellConfig.children as |child index|}}
    {{frost-bunsen-cell
      changeSet=propagatedChangeSet
      bunsenId=child.bunsenId
      bunsenModel=bunsenModel
      bunsenView=bunsenView
      cellConfig=child
      errors=diffError
      formDisabled=formDisabled
      formHook=formHook
      onChange=onChange
      readOnly=readOnly
      registerForFormValueChanges=registerForFormValueChanges
      renderers=renderers
      showAllErrors=showAllErrors
      value=diffValue
    }}
  {{/each}}
{{/if}}
